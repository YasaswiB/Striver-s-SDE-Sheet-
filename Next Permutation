
#include<iostream>
#include<vector>
#include <bits/stdc++.h>
using namespace std;
class Solution {
public:
    void nextPermutation(vector<int>& nums) {
        int n = nums.size(), k, l;
    	for (k = n - 2; k >= 0; k--) {
            if (nums[k] < nums[k + 1]) {
                break;
            }
        }
    	if (k < 0) {
    	    reverse(nums.begin(), nums.end());
    	} else {
    	    for (l = n - 1; l > k; l--) {
                if (nums[l] > nums[k]) {
                    break;
                }
            } 
    	    swap(nums[k], nums[l]);
    	    reverse(nums.begin() + k + 1, nums.end());
        }
    }
};


int main()
{
 
    
    vector<int> arr = { 1,2,3 };
 
    Solution s1;
    s1.nextPermutation(arr);
 
    for (int i=0;i<arr.size();i++) {
        cout << arr[i] << " ";
    }
 
    return 0;
}
